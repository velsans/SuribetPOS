<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingDefaultResource">

    <data>

        <variable
            name="cashout_transaction"
            type="com.suribetpos.main.ui.viewmodel.CashoutTransactionViewModel" />
    </data>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/textview_or_edittext_hight_large"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/cashouttrans_comments"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{cashout_transaction.}"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="@color/color_whit" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_gravity="left"
                android:background="@color/login_background" />

            <TextView
                android:id="@+id/cashouttrans_paid"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:gravity="center"
                android:text="@{cashout_transaction.}"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="@color/color_whit" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_gravity="left"
                android:background="@color/login_background" />

            <TextView
                android:id="@+id/cashouttrans_user"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.7"
                android:gravity="center"
                android:text="@{cashout_transaction.}"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="@color/color_whit"

                />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_gravity="left"
                android:background="@color/login_background" />

            <TextView
                android:id="@+id/cashouttrans_time"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:gravity="center"
                android:text="@{cashout_transaction.}"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="@color/color_whit" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="left"
            android:background="@color/login_background" />
    </LinearLayout>
</layout>