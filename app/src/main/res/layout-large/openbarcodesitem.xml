<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="openbarcodeResponceModel"
            type="com.suribetpos.main.ui.topup.model.OpenBarcodesModel" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/textview_or_edittext_hight"
        android:orientation="horizontal"
        android:visibility="visible">

        <TextView
            android:id="@+id/openbarcodes"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/margin_normal"
            android:layout_weight="1"
            android:background="@color/colorPrimary"
            android:gravity="center"
            android:text="@{openbarcodeResponceModel.barcode}"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/color_whit"
            android:textStyle="bold" />
    </LinearLayout>
</layout>