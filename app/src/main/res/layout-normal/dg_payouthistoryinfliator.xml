<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/textview_or_edittext_hight">

        <TextView
            android:id="@+id/payouthistory_barcode"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/oneColor"
            android:gravity="center" />

        <TextView
            android:id="@+id/payouthistory_stakeamt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/twoColor"
            android:gravity="center" />

        <TextView
            android:id="@+id/payouthistory_paidamt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/fiveColor"
            android:gravity="center" />

        <TextView
            android:id="@+id/payouthistory_by"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/threerColor"
            android:gravity="center" />

        <TextView
            android:id="@+id/payouthistory_time"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/fourColor"
            android:gravity="center" />

    </LinearLayout>
</LinearLayout>
